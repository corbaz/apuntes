{
    "name": "apuntes-jcc",
    "version": "1.0.0",
    "description": "JCC Producciones - Apuntes ",
    "main": "index.js",
    "scripts": {
        "build": "mkdocs build",
        "dev": "mkdocs serve",
        "GIT-ALL": "gz.bat",
        "update": "ncu -u && pnpm install",
        "git-init": "git init && npm run git-remote",
        "git-remote": "git remote add origin https://github.com/corbaz/apuntes.git",
        "git-add": "git add .",
        "git-commit": "git add .",
        "git-push": "git push -u origin main",
        "notebo-ok": "npm run build && cd dist && surge --domain apuntes.surge.sh",
        "corbaz": "npm run build && cd site && surge --domain mkdocs.surge.sh && cd .."
    },
    "author": "Julio Cesar Corbaz",
