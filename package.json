{
    "name": "apuntes-jcc",
    "version": "1.0.0",
    "description": "JCC Producciones - Apuntes ",
    "main": "index.js",
    "scripts": {
        "build": "cls && mkdocs build --clean",
        "dev": "cls && mkdocs serve",
        "GIT-ALL": "gz.bat && npm run mkdocs.surge.sh",
        "update": "ncu -u && pnpm install",
        "git-init": "git init && npm run git-remote",
        "git-remote": "git remote add origin https://github.com/corbaz/apuntes.git",
        "git-add": "git add .",
        "git-commit": "git commit -m ",
        "git-push": "git push -u origin main",
        "mkdocs.surge.sh": "cd docs && surge --domain mkdocs.surge.sh & cd ..",
        "corbaz": "cd docs && surge --domain corbaz.surge.sh && cd .."
    },
    "author": "Julio Cesar Corbaz",
    "license": "ISC",
    "devDependencies": {
        "vuepress": "^1.7.1"
    }
}